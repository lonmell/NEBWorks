<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer2"
    android:layout_width="300dp"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:ignore="HardcodedText,TouchTargetSizeCheck">

    <LinearLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:orientation="vertical"
        android:paddingBottom="10dp"
        tools:ignore="HardcodedText,TouchTargetSizeCheck">

        <ImageView
            android:id="@+id/close_btn"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="end"
            android:layout_marginStart="15dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/close_gray" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:gravity="center"
            android:orientation="horizontal">

            <androidx.cardview.widget.CardView
                android:id="@+id/user_profile_box"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                app:cardBackgroundColor="#dbdbdb"
                app:cardCornerRadius="60dp"
                app:cardElevation="0dp">

                <ImageView
                    android:id="@+id/user_profile"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY" />
            </androidx.cardview.widget.CardView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:layout_toStartOf="@+id/my_stting_area"
                android:layout_toEndOf="@+id/user_profile_box"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/user_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_bold"
                    android:gravity="start|center"
                    android:text="user_name"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:id="@+id/jikgup"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_regular"
                    android:gravity="start|center"
                    android:text="jikgup"
                    android:textColor="@color/blue"
                    android:textSize="13sp"
                    tools:ignore="HardcodedText" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/my_stting_area"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/my_setting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ic_setting_icon"
                    android:scaleType="fitXY" />
            </LinearLayout>
        </RelativeLayout>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="66dp"
            android:layout_below="@+id/close_btn"
            android:layout_marginStart="16dp"
            android:layout_marginTop="23dp"
            android:layout_marginEnd="15dp"
            android:padding="5dp"
            app:cardBackgroundColor="#EFF4FD"
            app:cardCornerRadius="5dp"
            app:cardElevation="0dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:gravity="center">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"
                    android:layout_toStartOf="@+id/refresh_area"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/store_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="start|center"
                        android:text="store_name"
                        android:textColor="@color/black"
                        android:textSize="17sp"
                        tools:ignore="HardcodedText" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/store_name"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="start|center"
                        android:text="현재 선택된 매장"
                        android:textColor="#696969"
                        android:textSize="13sp"
                        tools:ignore="HardcodedText" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/refresh_area"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="15dp"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/refresh_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ic_refresh_btn"
                        android:scaleType="fitXY" />
                </LinearLayout>
            </RelativeLayout>

        </androidx.cardview.widget.CardView>

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <!--area01-->
                <LinearLayout
                    android:id="@+id/area01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <!-- 매장정보 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="26dp"
                        android:layout_marginEnd="15dp"
                        android:drawableStart="@drawable/ic_circle_gray_check"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="  매장관리"
                        android:textColor="@color/black"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            app:cardBackgroundColor="#F5F6F8"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/select_nav01"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="전체 매장 목록"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />

                                <TextView
                                    android:id="@+id/select_nav02"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="신규 매장 등록"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

                <!--area02-->
                <LinearLayout
                    android:id="@+id/area02"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">
                    <!-- 직원관리 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="26dp"
                        android:layout_marginEnd="15dp"
                        android:drawableStart="@drawable/ic_circle_gray_check"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="  직원관리"
                        android:textColor="@color/black"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="90dp"
                            app:cardBackgroundColor="#F5F6F8"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/select_nav03"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="전체 직원 목록"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />

                                <TextView
                                    android:id="@+id/select_nav04"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="근무현황"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

                <!--area03-->
                <LinearLayout
                    android:id="@+id/area03"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <!-- 급여관리 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="26dp"
                        android:layout_marginEnd="15dp"
                        android:drawableStart="@drawable/ic_chatnchat"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="  급여관리"
                        android:textColor="@color/black"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="#F5F6F8"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/select_nav05"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="전체 급여관리"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="TouchTargetSizeCheck" />

                                <TextView
                                    android:id="@+id/select_nav06"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="급여 명세서 관리"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

                <!--area04-->
                <LinearLayout
                    android:id="@+id/area04"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">
                    <!-- 커뮤니티 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="26dp"
                        android:layout_marginEnd="15dp"
                        android:drawableStart="@drawable/ic_headphone"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="  출결관리"
                        android:textColor="@color/black"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            app:cardBackgroundColor="#F5F6F8"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/select_nav12_1"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="출결관리"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

                <!--area05-->
                <LinearLayout
                    android:id="@+id/area05"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <!-- 할일관리 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="26dp"
                        android:layout_marginEnd="15dp"
                        android:drawableStart="@drawable/ic_headphone"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="  할일관리"
                        android:textColor="@color/black"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="#F5F6F8"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/select_nav07"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="캘린더"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />

                                <TextView
                                    android:id="@+id/select_nav08"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="할일 추가하기"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />

                                <TextView
                                    android:id="@+id/select_nav09"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="결재처리"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

                <!--area06-->
                <LinearLayout
                    android:id="@+id/area06"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">
                    <!-- 커뮤니티 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="26dp"
                        android:layout_marginEnd="15dp"
                        android:drawableStart="@drawable/ic_headphone"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="  커뮤니티"
                        android:textColor="@color/black"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            app:cardBackgroundColor="#F5F6F8"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/select_nav12"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="커뮤니티"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

                <!--area07-->
                <LinearLayout
                    android:id="@+id/area07"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <!-- 근로계약관리 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="26dp"
                        android:layout_marginEnd="15dp"
                        android:drawableStart="@drawable/ic_headphone"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="  근로계약관리"
                        android:textColor="@color/black"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="#F5F6F8"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/select_nav10"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:text="전체 근로계약관리"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText,TouchTargetSizeCheck" />

                                <TextView
                                    android:id="@+id/select_nav11"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:drawableEnd="@drawable/ic_page_move_arrow"
                                    android:fontFamily="@font/inter_regular"
                                    android:gravity="start|center"
                                    android:onClick="btnOnclick"
                                    android:paddingStart="20dp"
                                    android:paddingEnd="20dp"
                                    android:visibility="gone"
                                    android:text="근로계약 작성"
                                    android:textColor="@color/black"
                                    android:textSize="13sp"
                                    tools:ignore="HardcodedText" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

                <!--area08-->
                <LinearLayout
                    android:id="@+id/area08"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <!-- 근로계약관리 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="26dp"
                        android:layout_marginEnd="15dp"
                        android:drawableStart="@drawable/ic_headphone"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="  고객센터"
                        android:textColor="@color/black"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="15dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
