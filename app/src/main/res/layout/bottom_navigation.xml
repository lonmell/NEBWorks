<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--@+id/bottom_navigation-->
    <LinearLayout
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#fff"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="5"
        app:layout_constraintBottom_toBottomOf="parent">

        <LinearLayout
            android:id="@+id/bottom_navigation01"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:onClick="btnOnclick"
            tools:ignore="SpeakableTextPresentCheck">

            <ImageView
                android:id="@+id/bottom_icon01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bottom_icon01"
                tools:ignore="ImageContrastCheck" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/bottom_navigation02"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:onClick="btnOnclick"
            tools:ignore="SpeakableTextPresentCheck">

            <ImageView
                android:id="@+id/bottom_icon02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bottom_icon02"
                tools:ignore="ImageContrastCheck" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/bottom_navigation03"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:onClick="btnOnclick"
            tools:ignore="SpeakableTextPresentCheck">

            <ImageView
                android:id="@+id/bottom_icon03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bottom_icon03"
                tools:ignore="ImageContrastCheck" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/bottom_navigation04"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:onClick="btnOnclick"
            tools:ignore="SpeakableTextPresentCheck">

            <ImageView
                android:id="@+id/bottom_icon04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bottom_icon04"
                tools:ignore="ImageContrastCheck" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/bottom_navigation05"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:onClick="btnOnclick"
            tools:ignore="SpeakableTextPresentCheck">

            <ImageView
                android:id="@+id/bottom_icon05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bottom_icon05"
                tools:ignore="ImageContrastCheck" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>