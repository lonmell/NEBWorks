                        android:layout_height="48dp"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:gravity="end|center"
                        android:visibility="invisible"
                        android:text="보기"
                        android:textColor="#696969"/>

                </RelativeLayout>

                <LinearLayout
