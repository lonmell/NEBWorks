                        android:text="인증번호 발송"
                        android:textColor="#66521E"
                        android:textSize="18sp"
                        tools:ignore="HardcodedText,TextContrastCheck" />
                </LinearLayout>


                <TextView
                    android:id="@+id/confirm_phone_btn"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/login_btn_round"
                    android:fontFamily="@font/applesdgothicneo"
                    android:gravity="center"
                    android:text="인증번호 확인"
                    android:textColor="#66521E"
                    android:textSize="18sp"
                    tools:ignore="HardcodedText,TextContrastCheck" />
            </LinearLayout>

            <TextView
                android:id="@+id/check_validation_txt2"
                android:layout_width="192dp"
                android:layout_height="wrap_content"
                android:text="@string/check_validation_String"
                android:textSize="12sp"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:textColor="#B1B1B1"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="8dp"
