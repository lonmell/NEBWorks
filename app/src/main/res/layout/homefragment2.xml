<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="UsingOnClickInXml">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="#F5F6F8">
            <!--place_info-->
            <androidx.cardview.widget.CardView
                android:id="@+id/place_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="25dp"
                    android:layout_marginBottom="10dp"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:id="@+id/item_area"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/i_o_btn"
                            android:layout_width="36dp"
                            android:layout_height="36dp"
                            android:layout_alignParentStart="true"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="15dp"
                            android:elevation="10dp"
                            app:cardBackgroundColor="@color/blue"
                            app:cardCornerRadius="60dp"
                            app:cardElevation="5dp">

                            <ImageView
                                android:id="@+id/store_thumnail"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#dcdcdc"
                                android:scaleType="fitXY" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/title"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignTop="@+id/i_o_btn"
                            android:layout_alignBottom="@+id/i_o_btn"
                            android:layout_marginStart="10dp"
                            android:layout_marginEnd="10dp"
                            android:layout_toStartOf="@+id/state"
                            android:layout_toEndOf="@+id/i_o_btn"
                            android:fontFamily="@font/inter_bold"
                            android:gravity="start|center"
                            android:maxLines="1"
                            android:text="Store name tv"
                            android:textColor="#000000"
                            android:textSize="16sp"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:id="@+id/state"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignTop="@+id/i_o_btn"
                            android:layout_alignBottom="@+id/i_o_btn"
                            android:layout_marginStart="10dp"
                            android:layout_marginEnd="30dp"
                            android:layout_toStartOf="@+id/place_state"
                            android:fontFamily="@font/inter_bold"
                            android:gravity="start|center"
                            android:maxLines="2"
                            android:text="현재 퇴근 중"
                            android:textColor="@color/blue"
                            android:textSize="13sp"
                            tools:ignore="HardcodedText" />

                        <ImageView
                            android:id="@+id/place_state"
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:layout_alignParentEnd="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/switching_store" />
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff" />

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/accept_area"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:layout_marginTop="10dp"
                android:visibility="gone"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="25dp"
                    android:layout_marginEnd="25dp"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/invite_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginStart="10dp"
                            android:layout_marginEnd="10dp"
                            android:fontFamily="@font/inter_regular"
                            android:layout_toStartOf="@+id/state"
                            android:layout_alignParentStart="true"
                            android:gravity="start|center"
                            android:text="매장 초대를 수락하시겠습니까?"
                            android:textColor="#000000"
                            android:textSize="16sp"
                            tools:ignore="HardcodedText" />

                        <TextView
                            android:id="@+id/accept_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentEnd="true"
                            android:layout_marginStart="10dp"
                            android:layout_marginEnd="10dp"
                            android:fontFamily="@font/inter_bold"
                            android:gravity="start|center"
                            android:paddingStart="10dp"
                            android:paddingEnd="10dp"
                            android:text="초대 수락"
                            android:textColor="@color/blue"
                            android:textSize="13sp"
                            tools:ignore="HardcodedText" />

                    </RelativeLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <!--location_state-->
            <androidx.cardview.widget.CardView
                android:id="@+id/location_state"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:layout_marginStart="21dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="21dp"
                android:layout_marginBottom="20dp"
                app:cardBackgroundColor="#FCF0EC"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                app:layout_constraintTop_toBottomOf="@+id/place_info">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignParentStart="true"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/inter_bold"
                            android:gravity="center"
                            android:text="매장과 "
                            android:textColor="@color/black"
                            android:textSize="14sp" />
                        <TextView
                            android:id="@+id/distance"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/inter_bold"
                            android:gravity="center"
                            android:text="0m"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:textColor="@color/black"
                            android:textSize="14sp" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/inter_bold"
                            android:gravity="center"
                            android:text=" 떨어져있습니다"
                            android:textColor="@color/black"
                            android:textSize="14sp" />
                    </LinearLayout>


                    <TextView
                        android:id="@+id/io_able_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignParentEnd="true"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="출근처리가능"
                        android:textColor="@color/red"
                        android:textSize="14sp" />
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <!--io_area-->
            <LinearLayout
                android:id="@+id/io_area"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="21dp"
                android:layout_marginEnd="21dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:weightSum="2"
                app:layout_constraintTop_toBottomOf="@+id/location_state">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center">

                    <ImageView
                        android:id="@+id/io_img"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_gravity="center"
                        android:background="@drawable/workinout01"
                        android:gravity="center" />
                </LinearLayout>
            </LinearLayout>

            <!--io_state-->
            <LinearLayout
                android:id="@+id/io_state"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                app:layout_constraintTop_toBottomOf="@+id/io_area">

                <TextView
                    android:id="@+id/io_time"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/inter_bold"
                    android:gravity="center"
                    android:text="오전 9시 40분"
                    android:textColor="@color/black"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/io_mytime"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/inter_bold"
                    android:gravity="center"
                    android:text="오늘 나의 근무시간 오전 10시 ~  오후 4시"
                    android:textColor="#696969"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="40dp"
                    android:layout_marginEnd="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="주간 근로시간"
                        android:textColor="#696969"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/week_worktime"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="1시간 70분"
                        android:textColor="#696969"
                        android:textSize="12sp" />

                    <SeekBar
                        android:id="@+id/week_worktime_seekbar"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_centerVertical="true"
                        android:layout_toStartOf="@+id/work_hour_text01"
                        android:layout_toEndOf="@+id/work_hour_text02"
                        android:max="60"
                        android:padding="5dp"
                        android:progress="20"
                        android:progressDrawable="@drawable/custom_seekbar_progress_week"
                        android:thumb="@drawable/seek_tumb" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="20dp"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="당월 근로시간"
                        android:textColor="#696969"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/month_worktime"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="1시간 70분"
                        android:textColor="#696969"
                        android:textSize="12sp" />

                    <SeekBar
                        android:id="@+id/month_worktime_seekbar"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_centerVertical="true"
                        android:layout_toStartOf="@+id/work_hour_text01"
                        android:layout_toEndOf="@+id/work_hour_text02"
                        android:max="60"
                        android:padding="5dp"
                        android:progress="45"
                        android:progressDrawable="@drawable/custom_seekbar_progress_week"
                        android:thumb="@drawable/seek_tumb" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="20dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="당월 근로시간"
                        android:textColor="#696969"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/month_iotcnt"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/inter_bold"
                        android:gravity="center"
                        android:text="1시간 70분"
                        android:textColor="#696969"
                        android:textSize="12sp" />

                    <SeekBar
                        android:id="@+id/month_iotcnt_seekbar"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_centerVertical="true"
                        android:layout_toStartOf="@+id/work_hour_text01"
                        android:layout_toEndOf="@+id/work_hour_text02"
                        android:max="60"
                        android:padding="5dp"
                        android:progress="30"
                        android:progressDrawable="@drawable/custom_seekbar_progress_week"
                        android:thumb="@drawable/seek_tumb" />
                </LinearLayout>
            </LinearLayout>
            <androidx.cardview.widget.CardView
                android:id="@+id/cardview01"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp"
                app:cardElevation="0dp"
                app:layout_constraintBottom_toTopOf="@+id/cardview02"
                app:layout_constraintTop_toBottomOf="@+id/io_state">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="공지"
                        android:textSize="14sp"
                        android:fontFamily="@font/inter_bold"
                        android:textColor="@color/blue"
                        android:gravity="center"/>
                    <TextView
                        android:id="@+id/limit_notitv"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="6월 주차권 공지"
                        android:textSize="14sp"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/inter_bold"
                        android:textColor="@color/black"
                        android:gravity="start|center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/cardview02"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="10dp"
                app:cardBackgroundColor="#ffffff"
                app:cardCornerRadius="10dp"
                app:cardElevation="0dp"
                app:layout_constraintBottom_toTopOf="@+id/bottom_menu">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="15dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:fontFamily="@font/inter_bold"
                            android:text="오늘의 업무"
                            android:textColor="@color/black" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:fontFamily="@font/inter_regular"
                            android:text="더보기 >"
                            android:textColor="#696969" />
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                            android:weightSum="5">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center"
                                android:text="완료"
                                android:textColor="#696969" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center"
                                android:text="미완료"
                                android:textColor="#696969" />

                            <LinearLayout
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:background="#dbdbdb" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center"
                                android:text="요청"
                                android:textColor="#696969" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center"
                                android:text="승인"
                                android:textColor="#696969" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center"
                                android:text="반려"
                                android:textColor="#696969" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                            android:weightSum="5">

                            <TextView
                                android:id="@+id/stateCnt05"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center|bottom"
                                android:text="0"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/stateCnt06"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center|bottom"
                                android:text="0"
                                android:textColor="@color/red" />

                            <LinearLayout
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:background="#dbdbdb" />

                            <TextView
                                android:id="@+id/stateCnt07"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center|bottom"
                                android:text="0"
                                android:textColor="#dbdbdb" />

                            <TextView
                                android:id="@+id/stateCnt08"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center|bottom"
                                android:text="0"
                                android:textColor="#dbdbdb" />

                            <TextView
                                android:id="@+id/stateCnt09"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_alignParentEnd="true"
                                android:layout_weight="1"
                                android:fontFamily="@font/inter_bold"
                                android:gravity="center|bottom"
                                android:text="0"
                                android:textColor="#dbdbdb" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/inter_bold"
                android:gravity="center|bottom"
                android:layout_marginStart="20dp"
                android:text="빠른이동"
                android:textColor="@color/black" />

            <LinearLayout
                android:id="@+id/bottom_menu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="20dp"
                android:orientation="horizontal"
                android:weightSum="4"
                app:layout_constraintBottom_toBottomOf="parent">

                <androidx.cardview.widget.CardView
                    android:id="@+id/member_management01"
                    android:layout_width="74dp"
                    android:layout_height="106dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="2dp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="#ffffff"
                    app:cardCornerRadius="10dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <androidx.cardview.widget.CardView
                            android:layout_width="43dp"
                            android:layout_height="43dp"
                            android:layout_above="@+id/tv01"
                            android:layout_centerHorizontal="true"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="5dp"
                            android:layout_marginBottom="15dp"
                            app:cardBackgroundColor="#dbdbdb"
                            app:cardCornerRadius="60dp">

                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/tv01"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_marginBottom="10dp"
                            android:fontFamily="@font/inter_bold"
                            android:gravity="center"
                            android:text="직원 관리"
                            android:textColor="@color/black" />
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/approval_go"
                    android:layout_width="74dp"
                    android:layout_height="106dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="2dp"
                    android:layout_weight="1"
                    app:cardBackgroundColor="#ffffff"
                    app:cardCornerRadius="10dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

